{
    "name": "@zzzz-/react-antd-extends",
    "description": "antd 组件二次封装",
    "version": "0.0.34",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/zkp442910864/webpack5-react/tree/antd-extends"
    },
    "homepage": "https://zkp442910864.github.com/zkp442910864/webpack5-react/tree/antd-extends",
    "scripts": {
        "start": "webpack serve --env CUSTOM_NODE_ENV=development --mode=development --config config/index.js",
        "build": "webpack build --env CUSTOM_NODE_ENV=production --mode=production --config config/index.js",
        "clear": "gulp run",
        "build:umd": "webpack build --env CUSTOM_NODE_ENV=production lib=umd --mode=production --config config/index.js",
        "build:es": "babel src --out-dir es --extensions .tsx --extensions .ts --ignore src/**/*.stories.tsx --ignore src/views --ignore src/types --ignore src/stories --ignore src/router --ignore src/App.tsx --ignore src/main.ts --source-maps --copy-files  --no-copy-ignored",
        "build:tsc-types": "tsc -p tsconfig.build.json",
        "build:lib": "npm run clear && npm run build:tsc-types && npm run build:es && npm run build:umd",
        "publish:patch": "npm version patch && npm run build:lib && npm publish",
        "copy:less": "copyfiles -u 1 src/**/*.scoped.less lib/",
        "jest": "jest --passWithNoTests",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "build:docs": "gulp clean-docs && npm run build-storybook && node ./.storybook/renamed.js"
    },
    "main": "dist/index.js",
    "module": "es/tsc.js",
    "types": "types/tsc.d.ts",
    "peerDependencies": {
        "antd": ">=3.0.0 || >=4.0.0",
        "babel-plugin-react-scoped-css": "1.1.1",
        "react": ">=16.0.0 || >=17.0.0",
        "react-dom": ">=16.0.0 || >=17.0.0",
        "scoped-css-loader": "^1.0.0"
    },
    "dependencies": {
        "antd": ">=3.0.0 || >=4.0.0",
        "axios": "^0.21.1",
        "history": "^5.3.0",
        "moment": "^2.29.1",
        "qs": "^6.10.1",
        "react": ">=16.0.0 || >=17.0.0",
        "react-dom": ">=16.0.0 || >=17.0.0",
        "react-redux": "^7.2.4",
        "react-router": "5.2.0",
        "react-router-dom": "5.2.0",
        "redux": "^4.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.6",
        "@babel/core": "^7.14.8",
        "@babel/plugin-transform-regenerator": "^7.14.5",
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/preset-env": "^7.14.8",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.14.5",
        "@babel/runtime": "^7.14.8",
        "@storybook/addon-actions": "^6.4.19",
        "@storybook/addon-docs": "^6.4.19",
        "@storybook/addon-essentials": "^6.4.19",
        "@storybook/addon-interactions": "^6.4.19",
        "@storybook/addon-links": "^6.4.19",
        "@storybook/builder-webpack5": "^6.4.19",
        "@storybook/cli": "^6.4.19",
        "@storybook/manager-webpack5": "^6.4.19",
        "@storybook/react": "^6.4.19",
        "@storybook/testing-library": "^0.0.9",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.0.0",
        "@testing-library/react-hooks": "^7.0.1",
        "@types/react": "^17.0.15",
        "@types/react-dom": "^17.0.9",
        "@types/react-router": "^5.1.16",
        "@types/react-router-dom": "^5.1.8",
        "@zzzz-/eslint-config-test": "^1.0.20",
        "@zzzz-/stylelint-config-test": "^1.0.9",
        "autoprefixer": "^10.3.1",
        "babel-loader": "^8.2.2",
        "babel-plugin-import": "^1.13.3",
        "babel-plugin-named-exports-order": "^0.0.2",
        "babel-plugin-react-scoped-css": "^1.1.1",
        "clean-webpack-plugin": "^4.0.0-alpha.0",
        "compression-webpack-plugin": "^8.0.1",
        "copy-webpack-plugin": "^9.0.1",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "css-loader": "^6.2.0",
        "css-minimizer-webpack-plugin": "^3.0.2",
        "del": "^6.0.0",
        "eslint": "8.10.0",
        "eslint-webpack-plugin": "^3.1.1",
        "extract-loader": "^5.1.0",
        "file-loader": "^6.2.0",
        "friendly-errors-webpack-plugin": "^1.7.0",
        "glob-all": "^3.2.1",
        "gulp": "^4.0.2",
        "html-loader": "^2.1.2",
        "html-webpack-plugin": "^5.3.2",
